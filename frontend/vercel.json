{
  "version": 2,
  "framework": "vite",
  "installCommand": "npm install",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "routes": [
    { "handle": "filesystem" },

 
    {
      "src": "/api/v1/(.*)",
      "dest": "https://seasky-backend.onrender.com/api/v1/$1"
    },

    {
      "src": "/api/(.*)",
      "dest": "https://seasky-backend.onrender.com/api/v1/$1"
    },


    {
      "src": "/media/(.*)",
      "dest": "https://seasky-backend.onrender.com/media/$1"
    },
    {
      "src": "/static/(.*)",
      "dest": "https://seasky-backend.onrender.com/static/$1"
    },


    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}